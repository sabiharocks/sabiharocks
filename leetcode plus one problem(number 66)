class Solution(object):
    def plusOne(self, digits):
        """
        :type digits: List[int]
        :rtype: List[int]
        """
        arr=[]
        count=str(0)
        for i in range(len(digits)):
            count+=str(digits[i])
        count1=int(count)+1
        for j in str(count1):
            arr.append(int(j))
        return arr
